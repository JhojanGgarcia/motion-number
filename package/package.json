{
	"name": "motion-number",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.1.0",
	"author": {
		"name": "Maxwell Barvian",
		"email": "max@barvian.me",
		"url": "https://barvian.me"
	},
	"description": "A React component to elegantly transition between numbers.",
	"license": "MIT",
	"homepage": "https://motion-number.barvian.me/",
	"repository": {
		"type": "git",
		"url": "https://github.com/barvian/motion-number",
		"directory": "src"
	},
	"bugs": {
		"url": "https://github.com/barvian/motion-number/issues"
	},
	"keywords": [
		"accessible",
		"react"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/es/index.mjs",
	"types": "./dist/cjs/index.d.ts",
	"files": [
		"dist",
		"README.md"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/es/index.d.mts",
				"default": "./dist/es/index.mjs"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		},
		"./config": {
			"import": {
				"types": "./dist/es/config.d.mts",
				"default": "./dist/es/config.mjs"
			},
			"require": {
				"types": "./dist/cjs/config.d.ts",
				"default": "./dist/cjs/config.js"
			}
		}
	},
	"scripts": {
		"build": "bunchee --tsconfig tsconfig.build.json",
		"dev": "bunchee --watch",
		"prepublishOnly": "pnpm run build && pnpm run test"
	},
	"peerDependencies": {
		"framer-motion": "^11.3.19",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"bunchee": "^5.2.2",
		"framer-motion": "^11.3.19",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	}
}
